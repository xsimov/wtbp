<%= form_for [@band, @concert] do |f| %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :hour %>
  <%= f.text_field :hour %>
  <%= f.label :place %>
  <%= f.text_field :place %>
  <%= f.label :additional_info %>
  <%= f.text_field :additional_info %>
  <%= f.submit "Save concert!" %>
<% end %>