<%= form_for [@band, @concert] do |f| %>
  <%= f.label :date %><br />
  <%= f.text_field :date %><br />
  <%= f.label :hour %><br />
  <%= f.text_field :hour %><br />
  <%= f.label :place %><br />
  <%= f.text_field :place %><br />
  <%= f.label :additional_info %><br />
  <%= f.text_field :additional_info %><br />
  <%= f.submit "Save concert!" %>
<% end %>